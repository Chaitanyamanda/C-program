
#include <stdio.h>
int main() {
    float a, b, c,d, result;
    printf("Enter value of a:\n");
    scanf("%f", &a);
    printf("Enter value of b:\n");
    scanf("%f", &b);
    printf("Enter value of c:\n");
    scanf("%f", &c);
    printf("Enter value of d:\n");
    scanf("%f", &d);
    result = (a + b) / (c + d);
    printf("Result = %f\n", result);
}
